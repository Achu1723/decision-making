# Budget

A=int(input())
B=int(input())
C=int(input())
D=int(input())
E=int(input())
p=A*350.34
q=B*230.90
r=C*190.55
s=D*125.30
t=E*180.90
z=p+q+r+s+t
if z<=15000:
    print("yes")
else:
    print("no")
    
# Sece Dining

x=input()
y=int(input())
if(x=='front'and y==1) or (x=='rear'and y==2):
    print("left handed")
elif(x=='front'and y==2) or (x=='rear'and y==1):
    print("right handed")
else:
    print("invalied input")
    
# Intersection of a Circle

import math
x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
d=math.sqrt(((x1-x2)**2)+((y1-y2)**2))
if d==r1+r2:
   print("The circles are tangential")
elif d<r1+r2:
   print("The circles overlap")
else:
    print("The circles are not overlap")
   
# Microwave Oven

print("Enter the number of items")
A=int(input())
print("Enter the single item heating time")
B=float(input())
X=B+(50/100)*B
Y=B+2*B
if A==2:
    print("The recommended heating time is %.2f"%X)
elif A==3:
    print("The recommended heating time is %.2f"%Y)
else:
    print("Number of items is more")
    

     
